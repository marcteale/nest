# Required.  Do not change.
[nest]

# Temperature scale.  Valid values:
#   Celsius: c
#   Fahrenheit: f
#   Kelvin: k (because why not)
#
# If no scale is specified, default to Celsius.
scale = f

# Output format.  Valid values:
#   json
#   csv
#   observium
#   rrdtool
format = observium

# Output file.  Defaults to stdout.
# outfile = myoutfile.txt
