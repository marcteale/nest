# Required.  Do not change.
[nest]

# Nest account email address
username = someone@example.com

# Password
password = badpassword

# Temperature scale.  Valid values:
#   Celsius: c
#   Fahrenheit: f
#   Kelvin: k (because why not)
#
# If no scale is specified, default to Celsius.
scale = f

# Output format.  Valid values:
#   json
#   csv
#   observium
#   rrdtool
output = observium
